#include <iostream>
#include <vector>
#include <string>
#include <string_view>
#include <cctype>

bool check_symb(std::string str) {
    return std::all_of(str.begin(), str.end(), [](char symb) {return !(symb >= 97 && symb <= 122); });
}

int main()
{
    std::string str;
    std::getline(std::cin, str);
    
    if (check_symb(str)) {
        std::cout << "YES" << std::endl;
    }
    else {
        std::cout << "no" << std::endl;
    }
}
