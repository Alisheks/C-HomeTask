#include <iostream>
#include <algorithm>
#include <cctype>

bool isUpperCase(const std::string& str) {
    return std::all_of(str.begin(), str.end(), [](char c) {
        return std::isupper(c);
    });
}

int main() {
    std::string str;

    std::getline(std::cin, str);

    if (isUpperCase(str)) {
        std::cout << "YES" << std::endl;
    } else {
        std::cout << "NO" << std::endl;
    }

    return 0;
}
