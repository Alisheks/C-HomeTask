#include <iostream>
#include <algorithm>
#include <string>

void moveSpacesToEnd(std::string& str) {
    // Перемещаем все пробелы в конец строки
    std::stable_partition(str.begin(), str.end(), [](char c) {
        return c != ' ';
    });

    // Добавляем кавычки к строке
    str.insert(str.begin(), '"');
    str.push_back('"');
}

int main() {
    std::string str;

    std::cout << "String: ";
    std::getline(std::cin, str);

    moveSpacesToEnd(str);

    std::cout << str << std::endl;

    return 0;
}
