#include <iostream>
#include <vector>
#include <algorithm>

void rev(std::vector<std::string>& vec) {
    // Обращаем порядок элементов вектора
    std::reverse(vec.begin(), vec.end());

    // Обращаем каждую строку вектора
    std::for_each(vec.begin(), vec.end(), [](std::string& str) {
        std::reverse(str.begin(), str.end());
    });
}

int main() {
    std::vector<std::string> vec = {"cat", "dog", "mouse", "elephant"};

    rev(vec);

    for (int i = 0; i < vec.size(); i++) {
        std::cout << vec[i] << std::endl;
    }

    return 0;
}
